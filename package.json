  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "arrow-body-style": "warn",
      "dot-notation": "warn",
      "no-var": "error",
      "no-debugger": "error",
      "no-duplicate-imports": "error",
      "no-multi-spaces": "warn",
      "object-shorthand": "error",
      "eqeqeq": [
        "error",
        "smart"
      ],
      "prefer-arrow-callback": "warn",
      "space-before-function-paren": [
        "warn",
        {
          "anonymous": "ignore",
          "named": "never",
          "asyncArrow": "always"
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
